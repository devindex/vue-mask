{
  "name": "@devindex/vue-mask",
  "version": "2.0.0-dev",
  "description": "Mask Plugin for Vue 3",
  "author": "Sergio Rodrigues",
  "main": "dist/vue-mask.ssr.js",
  "browser": "dist/vue-mask.esm.js",
  "module": "dist/vue-mask.esm.js",
  "unpkg": "dist/vue-mask.min.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "vue-cli-service serve src-doc/main.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "release": "bash build/release.sh"
  },
  "keywords": [
    "vue",
    "mask",
    "plugin"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "@vue/compiler-sfc": "^3.0.5",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.2.3",
    "rollup": "^2.36.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.35.2",
    "sass-loader": "^8.0.2",
    "spectre.css": "^0.5.9",
    "string-mask": "^0.3.0",
    "vue": "^3.0.5",
    "vue3-highlightjs": "^1.0.5"
  },
  "homepage": "https://github.com/devindex/vue-mask#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devindex/vue-mask.git"
  },
  "bugs": {
    "url": "https://github.com/devindex/vue-mask/issues"
  }
}
